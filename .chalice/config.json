{
    "version": "2.0",
    "app_name": "boost",
    "tags": {
        "Environment": "Development",
        "Application": "Boost"
    },
    "xray": true,
    "lambda_memory_size": 256,
    "stages": {
        "dev": {
            "api_gateway_stage": "api",
            "lambda_timeout": 240,
            "tags": {
                "Environment": "Development"
            },
            "environment_variables": {
              "CHALICE_STAGE": "dev"
            }
        },
        "test": {
            "api_gateway_stage": "api",
            "lambda_timeout": 240,
            "tags": {
                "Environment": "Test"
            },
            "environment_variables": {
              "CHALICE_STAGE": "test"
            }
        },
        "staging": {
            "api_gateway_stage": "api",
            "lambda_timeout": 240,
            "tags": {
                "Environment": "Staging"
            },
            "environment_variables": {
              "CHALICE_STAGE": "staging"
            }
        },
        "prod": {
            "api_gateway_stage": "api",
            "lambda_timeout": 240,
            "environment_variables": {
              "CHALICE_STAGE": "prod"
            }
        }
    },
  "include": ["prompts"]
}
